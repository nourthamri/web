<h1>List of Authors</h1>
<table border="1">
  <tr>
    <th>Username</th>
    <th>Email</th>
     <th>Actions</th>
  </tr>
  
  {% for a in author %}
    <tr>
      <td>{{ a.username }}</td>
        <td>{{ a.email}}</td>
       <td>
        <a href="{{ path('app_edit', { 'id': a.id }) }}">Edit</a>
      </td>
       <td>
    <a href="{{path ('app_delete',{ 'id': a.id })}}">Delete</a>
</td>
  {% endfor %}
  
</table>
